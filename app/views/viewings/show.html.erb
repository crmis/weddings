<div class="main-form">
  <fieldset>
    <h3>Viewing Confirmation</h3>
    <p>Your viewing is confirmed for the
      <%= @room.name %>:
      <%= @viewing.start_time.strftime('%e %b %Y %H:%M%p') %>
      until
      <%= (@viewing.calculate_end_time + 60).strftime('%e %b %Y %H:%M%p') %>
  </fieldset>
</div>
